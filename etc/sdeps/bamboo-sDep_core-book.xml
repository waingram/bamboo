 <!-- Method Map Datastream -->
  <foxml:datastream CONTROL_GROUP="X" ID="METHODMAP" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion ID="METHODMAP1.0"
    	FORMAT_URI="info:fedora/fedora-system:FedoraSDepMethodMap-1.1"
    	LABEL="Deployment Method Map" MIMETYPE="text/xml">
      <foxml:xmlContent>
        <fmm:MethodMap bDefPID="bamboo-sDef:core" name="MethodMap Core SDep" xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap">
          <fmm:Method operationName="getPreview" wsdlMsgName="getPreviewRequest" wsdlMsgOutput="dissemResponse">
            <fmm:DefaultInputParm defaultValue="$PID" label="The PID of the data object" parmName="PID" passBy="VALUE" required="false"/>
            <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="image/jpeg"/>
          </fmm:Method>
          <fmm:Method operationName="getAsHTML" wsdlMsgName="getAsHTMLRequest" wsdlMsgOutput="dissemResponse">
            <fmm:DefaultInputParm defaultValue="$PID" label="The PID of the data object" parmName="PID" passBy="VALUE" required="false"/>
            <fmm:DatastreamInputParm defaultValue="" label="URL to TEI Header" parmName="descMetadata" passBy="URL_REF" required="true"/>
            <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/html"/>
          </fmm:Method>
          <fmm:Method operationName="getEmbeddedContent" wsdlMsgName="getEmbeddedContentRequest" wsdlMsgOutput="dissemResponse">
            <fmm:DefaultInputParm defaultValue="$PID" label="The PID of the data object" parmName="PID" passBy="VALUE" required="true"/>
            <fmm:DatastreamInputParm defaultValue="" label="URL to TEI Header" parmName="descMetadata" passBy="URL_REF" required="true"/>
            <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/html"/>
          </fmm:Method>
          <fmm:Method operationName="getAsMETS" wsdlMsgName="getAsMETSRequest" wsdlMsgOutput="dissemResponse">
            <fmm:DefaultInputParm defaultValue="$PID" label="The PID of the data object" parmName="PID" passBy="VALUE" required="true"/>
            <fmm:DatastreamInputParm defaultValue="" label="URL to TEI Header" parmName="descMetadata" passBy="URL_REF" required="true"/>
            <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/xml"/>
          </fmm:Method>
          <fmm:Method operationName="getAsAtom" wsdlMsgName="getAsAtomRequest" wsdlMsgOutput="dissemResponse">
            <fmm:DefaultInputParm defaultValue="$PID" label="The PID of the data object" parmName="PID" passBy="VALUE" required="true"/>
            <fmm:DatastreamInputParm defaultValue="" label="URL to TEI Header" parmName="descMetadata" passBy="URL_REF" required="true"/>
            <fmm:MethodReturnType wsdlMsgName="dissemResponse" wsdlMsgTOMIME="text/xml"/>
          </fmm:Method>
        </fmm:MethodMap>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>

 <!-- Datastream Input Specification Datastream -->
  <foxml:datastream CONTROL_GROUP="X" ID="DSINPUTSPEC" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion ID="DSINPUTSPEC1.0"
        MIMETYPE="text/xml"
        FORMAT_URI="info:fedora/fedora-system:FedoraDSInputSpec-1.1"
        LABEL="Datastream Input Specification">
      <foxml:xmlContent>
        <fbs:DSInputSpec xmlns:fbs="http://fedora.comm.nsdlib.org/service/bindspec"
           bDefPID="bamboo-sDef:core"  label="Datastream Input Specification">
          <fbs:DSInput DSMax="1" DSMin="1" DSOrdinality="false" wsdlMsgPartName="descMetadata">
            <fbs:DSInputLabel>metadata Binding</fbs:DSInputLabel>
            <fbs:DSMIME>text/xml</fbs:DSMIME>
            <fbs:DSInputInstruction/>
          </fbs:DSInput>
        </fbs:DSInputSpec>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>


 <!-- WSDL Datastream -->
  <foxml:datastream CONTROL_GROUP="X" ID="WSDL" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion ID="WSDL1.0" LABEL="WSDL Bindings" MIMETYPE="text/xml">
      <foxml:xmlContent>
        <wsdl:definitions name="definitions"
			targetNamespace="urn:thisNamespace"
			xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" 
			xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
			xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap"
			xmlns:soapenc="http://schemas.xmlsoap.org/wsdl/soap/encoding" xmlns:this="sdep"
			xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

		  <!-- WSDL Types -->
          <wsdl:types>
            <xsd:schema targetNamespace="urn:thisNamespace">
			<xsd:simpleType name="PIDType">
			   <xsd:restriction base="xsd:string"/>
			</xsd:simpleType>
              <xsd:simpleType name="descMetadataType">
                <xsd:restriction base="xsd:string"/>
              </xsd:simpleType>
            </xsd:schema>
          </wsdl:types>

		  <!-- WSDL Messages -->

          <wsdl:message name="getPreviewRequest">
			<wsdl:part name="PID" type="this:PIDType"/>
          </wsdl:message>
          <wsdl:message name="getAsHTMLRequest">
			<wsdl:part name="PID" type="this:PIDType"/>
            <wsdl:part name="descMetadata" type="this:descMetadataType"/>
          </wsdl:message>
          <wsdl:message name="getEmbeddedContentRequest">
			<wsdl:part name="PID" type="this:PIDType"/>
            <wsdl:part name="descMetadata" type="this:descMetadataType"/>
          </wsdl:message>
          <wsdl:message name="getAsMETSRequest">
			<wsdl:part name="PID" type="this:PIDType"/>
            <wsdl:part name="descMetadata" type="this:descMetadataType"/>
          </wsdl:message>
          <wsdl:message name="getAsAtomRequest">
			<wsdl:part name="PID" type="this:PIDType"/>
            <wsdl:part name="descMetadata" type="this:descMetadataType"/>
          </wsdl:message>

          <wsdl:message name="dissemResponse">
            <wsdl:part name="dissem" type="xsd:base64Binary"/>
          </wsdl:message>

		  <!-- WSDL Port Type -->
          <wsdl:portType name="BambooBookCorePortType">
            <wsdl:operation name="getPreview">
              <wsdl:input message="this:getPreviewRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getAsHTML">
              <wsdl:input message="this:getAsHTMLRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getEmbeddedContent">
              <wsdl:input message="this:getEmbeddedContentRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getAsMETS">
              <wsdl:input message="this:getAsMETSRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getAsAtomRequest">
              <wsdl:input message="this:getAsAtomRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
          </wsdl:portType>

		  <!-- WSDL Service -->
          <wsdl:service name="BambooBookCore">
            <wsdl:port binding="this:BambooBookCore_http" name="BambooBookCore_port">
             <http:address location="LOCAL"/>
            </wsdl:port>
          </wsdl:service>

		  <!-- WSDL Binding -->
          <wsdl:binding name="BambooBookCore_http" type="this:BambooBookCorePortType">
            <http:binding verb="GET"/>
            <wsdl:operation name="getPreview">
              <http:operation location="http://wiki.projectbamboo.org/download/attachments/17629185/global.logo"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="image/jpeg"/>
              </wsdl:output>            
            </wsdl:operation>
            <wsdl:operation name="getAsHTML">
              <http:operation location="http://localhost:8080/saxon/SaxonServlet?style=http://localhost/xsl/general/core_book_html.xsl&amp;source=(descMetadata)&amp;pid=(PID)&amp;clear-stylesheet-cache=yes"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/html"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="getEmbeddedContent">
              <http:operation location="http://localhost:8080/saxon/SaxonServlet?style=http://localhost/xsl/general/core_book_embedded_html.xsl&amp;source=(descMetadata)&amp;pid=(PID)&amp;clear-stylesheet-cache=yes"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/html"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="getAsMETS">
              <http:operation location="http://localhost:8080/saxon/SaxonServlet?style=http://localhost/xsl/general/core_book_mets.xsl&amp;source=(descMetadata)&amp;pid=(PID)&amp;clear-stylesheet-cache=yes"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/xml"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="getAsAtom">
              <http:operation location="http://localhost:8080/saxon/SaxonServlet?style=http://localhost/xsl/general/core_book_atom.xsl&amp;source=(descMetadata)&amp;pid=(PID)&amp;clear-stylesheet-cache=yes"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/xml"/>
              </wsdl:output>
            </wsdl:operation>

          </wsdl:binding>
        </wsdl:definitions>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>

	<foxml:datastream ID="SERVICE-PROFILE" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
	<foxml:datastreamVersion ID="SERVICE-PROFILE1.0" LABEL="Service Profile - Technical description of the service" MIMETYPE="text/xml">
	<foxml:xmlContent>
	<fsvp:serviceProfile xmlns:fsvp="http://fedora.comm.nsdlib.org/service/profile" bDefPID="bamboo-sDef:core" name="core">
			  <fsvp:serviceDescription>Core Behavior for Bamboo Objects</fsvp:serviceDescription>
			  <fsvp:serviceImplementation>
				<fsvp:serviceLiveTestURL>http://localhost:8080/saxon/SaxonServlet</fsvp:serviceLiveTestURL>
	
				<fsvp:serviceMessagingProtocol>HTTP GET</fsvp:serviceMessagingProtocol>
				<fsvp:serviceInputFormats>
				  <fsvp:MIMEType>text/xml</fsvp:MIMEType>
				</fsvp:serviceInputFormats>
				<fsvp:serviceOutputFormats>
				  <fsvp:MIMEType>text/xml</fsvp:MIMEType>
				  <fsvp:MIMEType>text/html</fsvp:MIMEType>
				  <fsvp:MIMEType>image/jpeg</fsvp:MIMEType>
				</fsvp:serviceOutputFormats>
				<fsvp:serviceImplDependencies>
				  <fsvp:software license="Mozilla_Public_License" name="Tomcat" opensource="true" type="Software_Library" version="5.5"></fsvp:software>
				  <fsvp:software license="MPL" name="SaxonServlet" opensource="true" type="Servlet" version="9.0.0.6"></fsvp:software>
				</fsvp:serviceImplDependencies>
			  </fsvp:serviceImplementation>
	
			</fsvp:serviceProfile>
	</foxml:xmlContent>
	</foxml:datastreamVersion>
	</foxml:datastream>
</foxml:digitalObject>
