      <foxml:xmlContent>
        <fmm:MethodMap name="MethodMap - Addressable Image BDEF" xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap">
          <fmm:Method label="Get thumbnail image." operationName="getThumbnail"/>
          <fmm:Method label="Get screen-size image." operationName="getScreenSize"/>
          <fmm:Method label="Get given crop with given width." operationName="getCropWithWidth">
            <fmm:UserInputParm defaultValue="" label="x position of crop" parmName="x" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="y position of crop" parmName="y" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="width of crop" parmName="width" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="height of crop" parmName="height" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="desired width" parmName="destwidth" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Get given crop with given height." operationName="getCropWithHeight">
            <fmm:UserInputParm defaultValue="" label="x position of crop" parmName="x" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="y position of crop" parmName="y" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="width of crop" parmName="width" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="height of crop" parmName="height" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="desired height" parmName="destheight" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Get given crop with given size." operationName="getCropWithSize">
            <fmm:UserInputParm defaultValue="" label="x position of crop" parmName="x" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="y position of crop" parmName="y" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="width of crop" parmName="width" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="height of crop" parmName="height" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="desired width" parmName="destwidth" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="desired height" parmName="destheight" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Get image with given width and height." operationName="getWithSize">
            <fmm:UserInputParm defaultValue="" label="Desired width" parmName="width" passBy="VALUE" required="true"/>
            <fmm:UserInputParm defaultValue="" label="Desired height" parmName="height" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Get image with given width." operationName="getWithWidth">
            <fmm:UserInputParm defaultValue="" label="Desired width" parmName="width" passBy="VALUE" required="true"/>
