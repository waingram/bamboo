          <fmm:Method label="Get image with given height." operationName="getWithHeight">
            <fmm:UserInputParm defaultValue="" label="Desired height" parmName="height" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Get image with given longest side." operationName="getWithLongSide">
            <fmm:UserInputParm defaultValue="" label="Desired longest side" parmName="length" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Re-process the image for delivery." operationName="reProcess"/>
