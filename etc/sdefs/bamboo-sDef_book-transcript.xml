 <!-- Method Map Datastream -->
  <foxml:datastream CONTROL_GROUP="X" ID="METHODMAP" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion ID="METHODMAP1.0"
    	FORMAT_URI="info:fedora/fedora-system:FedoraSDefMethodMap-1.0"
    	LABEL="Abstract Method Map" MIMETYPE="text/xml">

      <foxml:xmlContent>
        <fmm:MethodMap name="MethodMap - Book Transcript Behavior Definition" xmlns:fmm="http://fedora.comm.nsdlib.org/service/methodmap">
          <fmm:Method label="get teiHeader" operationName="getHeader"/>
          <fmm:Method label="Get TOC as div structure" operationName="getTOC"/>
          <fmm:Method label="get TOC by Type" operationName="getTOCByType">
            <fmm:UserInputParm defaultValue="" label="div type" parmName="type" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get front matter" operationName="getFront"/>
          <fmm:Method label="get xml fragment with id attribute" operationName="getChunk">
            <fmm:UserInputParm defaultValue="" label="xml element id attribute value" parmName="xmlid" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get xml element by name" operationName="getElementsByName">
            <fmm:UserInputParm defaultValue="" label="xml element name" parmName="elementName" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get head element for &lt;div> given &lt;div> id attribute" operationName="getHeading">
            <fmm:UserInputParm defaultValue="" label="div element id attribute value" parmName="xmlid" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get xml between ordinal and following &lt;pb> inclusive" operationName="getPageByNumber">
            <fmm:UserInputParm defaultValue="" label="page ordinal" parmName="pagenumber" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get xml after &lt;pb> with facs attribute" operationName="getPageByFacs">
            <fmm:UserInputParm defaultValue="" label="&lt;pb> facs" parmName="facsId" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get text between ordinal and following &lt;pb> inclusive" operationName="getPageTextByNumber">
            <fmm:UserInputParm defaultValue="" label="page ordinal" parmName="pagenumber" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="get text after &lt;pb> with facs attribute" operationName="getPageTextByFacs">
            <fmm:UserInputParm defaultValue="" label="&lt;pb> facs" parmName="facsId" passBy="VALUE" required="true"/>
          </fmm:Method>
          <fmm:Method label="Reindex the TEI datastream" operationName="refresh"/>
        </fmm:MethodMap>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>
